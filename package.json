{
  "name": "react-hook-redux-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bloomer": "^0.6.5",
    "bulma": "^0.7.4",
    "emotion": "^10.0.9",
    "node-sass": "^4.11.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^5.0.0",
    "react-scripts-ts": "3.1.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.0",
    "redux-react-hook": "^3.2.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/assets/scss/ -o src/assets/scss/",
    "watch-css": "npm run build-css && node-sass-chokidar src/assets/scss/ -o src/assets/scss/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-ts",
    "start-ts": "react-scripts-ts start",
    "build": "npm-run-all -p build-css build-ts",
    "build-ts": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/history": "^4.7.2",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.0",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "@types/react-helmet": "^5.0.8",
    "@types/react-router": "^4.4.5",
    "prettier": "^1.16.4",
    "typescript": "^3.4.1"
  }
}
